{
    "manifest_version": 2,
    "name": "Linkedin-Automator",
    "version": "0.0.1",
    "content_scripts": [
      {
        "matches": [
          "https://www.linkedin.com/*",
          "http://www.linkedin.com/*"
        ],
        "js": ["content.js"]
      }
    ],
    "permissions": [],
    "browser_action": {
      "default_icon": "in.png",
      "default_popup": "popups/index.html",
      "default_title": "Automate Linkedin!"
    }
  }